{
  "cli": {
    "version": ">= 12.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "staging": {
      "channel": "staging"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_KEY": "public",
        "EXPO_PUBLIC_GOOGLE_MAPS_KEY": "public"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      } 
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "dhitiris@yahoo.gr",
        "ascAppId": "6670754535",
        "appleTeamId": "ZMF2MQZHPK"
      }
    }
  }
}
